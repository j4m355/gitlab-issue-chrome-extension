{
	"manifest_version": 2,
	"name": "Gitlab-Issue",
	"description": "Raise issues with screenshots",
	"version": "0.0.1",
	"browser_action": {
	    "default_icon": "icon.png",
	    "default_popup": "background.html"
  	},
  	"options_page": "options.html",
  	"permissions" : [
  		"tabs",
  		"<all_urls>",
  		"unlimitedStorage"
  	]
 }

